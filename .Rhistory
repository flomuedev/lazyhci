DV.pretty <- DV
data <- fix_col_names(data)
IVs <- fix_strings(IVs)
DV <- fix_strings(DV)
data.plot <- data_summary.internal(data, DV, IVs, fun.sum, fun.error, na.rm = na.rm)
p<- NULL
if(length(IVs) == 1)
p<- ggplot(data.plot, aes_string(x=IVs[1], y="mean", fill=IVs[1])) + xlab(IVs.pretty[1])
else
